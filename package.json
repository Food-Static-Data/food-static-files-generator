{
  "name": "@groceristar/static-data-generator",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "bundle": "rollup -c",
    "dev": "rollup -c -w",
    "build": "npm run clean && npm run bundle",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "zprepublish": "yarn clean && yarn lint && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint ./src --fix",
    "format": "prettier ./src/**",
    "formato": "pretty-quick --staged",
    "play": "node ./src/play/play.js",
    "build:babel": "babel src -d dist",
    "debug:generator": "rollup -c && node ./debug/runGenerator.js",
    "debug:write": "rollup -c && node ./debug/runWrite.js"
  },
  "description": "module to use static JSON files with groceristar projects",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "repository": "https://github.com/GroceriStar/food-static-files-generator",
  "author": "Arthur Tkachenko",
  "private": false,
  "release": {
    "publishDir": "dist"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "coveralls": "3.0.5",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.13.0",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "regenerator-runtime": "0.13.3",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "shx": "0.3.2"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dayjs": "^1.8.13",
    "fs": "^0.0.2",
    "lodash": "^4.17.11",
    "path": "^0.12.7",
    "path-exists": "^4.0.0",
    "randombytes": "^2.1.0",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean && npm run formato",
      "after": "lint-staged",
      "then": "yarn precommit"
    }
  },
  "files": [
    "dist"
  ]
}
